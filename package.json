{
  "name": "rx-subscribe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "cross-env CI=true npm run test",
    "lint": "ng lint",
    "format": "prettier --write projects/**/*.{ts,html,css}",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "copy:docs": "cp *.md dist/rx-subscribe",
    "release:first": "npm run release -- --first-release",
    "build:lib": "ng build rx-subscribe --prod && npm run copy:docs",
    "test:lib": "ng test rx-subscribe",
    "release": "cd projects/rx-subscribe && standard-version --infile ../../CHANGELOG.md",
    "test:lib:headless": "cross-env CI=true npm run test:lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.1",
    "@angular/common": "9.0.1",
    "@angular/compiler": "9.0.1",
    "@angular/core": "9.0.1",
    "@angular/forms": "9.0.1",
    "@angular/platform-browser": "9.0.1",
    "@angular/platform-browser-dynamic": "9.0.1",
    "@angular/router": "9.0.1",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.1",
    "@angular-devkit/build-ng-packagr": "0.901.1",
    "@angular/cli": "9.1.1",
    "@angular/compiler-cli": "9.0.1",
    "@angular/language-service": "9.0.1",
    "@types/node": "12.12.34",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "codelyzer": "5.2.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "ng-packagr": "9.0.3",
    "protractor": "5.4.3",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "typescript": "3.7.5",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "git-cz": "4.3.1",
    "all-contributors-cli": "6.14.1",
    "lint-staged": "10.1.2",
    "prettier": "2.0.4",
    "standard-version": "7.1.0",
    "husky": "4.2.3",
    "cross-env": "7.0.2",
    "@ngneat/spectator": "5.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged",
      "pre-push": "npm run test:headless"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
